<?xml version="1.0" encoding="UTF-8"?>
<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0" immediate="true" name="org.osdfreactive.policyparser.DefaultPolicyParser" activate="activate" deactivate="deactivate" modified="modified">
    <implementation class="org.osdfreactive.policyparser.DefaultPolicyParser"/>
